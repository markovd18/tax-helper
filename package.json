{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/refs/heads/master/src/schemas/json/package.json",
  "name": "tax-helper",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm build:types && pnpm build:js",
    "build:types": "tsc --noEmit",
    "build:js": "vite build",
    "preview": "vite preview",
    "lint:format": "prettier \"./**/*.{js,jsx,ts,tsx,svg,json}\" --list-different",
    "lint": "pnpm lint:format",
    "fix": "pnpm fix:format",
    "fix:format": "pnpm lint:format --write"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^3.4.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^4.7.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  }
}
